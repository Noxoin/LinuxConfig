runtime! debian.vim
"syntax on
set tabstop=8
set hlsearch
set softtabstop=4
set shiftwidth=4
set expandtab
set autoindent
set ruler
"set ls=2
set background=light

" 80 column limit highlight
"highlight ExtraWhitespace ctermbg=green guibg=blue ctermfg=white
"match ExtraWhitespace /\s\+$\| \+\ze\t\|\%81v.*/
